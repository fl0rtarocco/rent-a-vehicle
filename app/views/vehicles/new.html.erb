<div class="container">
  <div class="containertwo" style="margin: 100px auto;
    padding: 10px;">
    <%= simple_form_for @vehicle do |f| %>
      <%= f.input :name %>
      <%= f.input :price %>
      <%= f.input :description %>
      <%= f.input :address %>
      <%= f.select :category, ["Elevator", "Car", "Motorcycle", "Scooter", "Giraffe", "Truck", "Tanks", "Private Jet"] %>
      <%= f.input :photos, as: :file, input_html: {multiple: true} %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
